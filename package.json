{
  "scripts": {
    "test": "nyc --reporter=text mocha --inspect --require babel-register --require source-map-support --recursive",
    "build": "babel src --out-dir js",
    "prepare": "npm run build && webpack --optimize-minimize",
    "watch": "babel --watch src --out-dir js",
    "coverage": "nyc report --reporter=text-lcov",
    "prepublish": "npm run prepare"
  },
  "main": "js/index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-do-expressions",
      ["@babel/plugin-proposal-pipeline-operator", { "proposal": "minimal" }],
      "lodash",
      "macros"
    ]
  },
  "name": "styled-tachyons",
  "version": "1.0.3",
  "description": "Mix tachyons into styled-components.",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linonetwo/styled-tachyons.git"
  },
  "keywords": [
    "tachyons",
    "styled-components",
    "css",
    "stylesheet"
  ],
  "author": "lin onetwo",
  "bugs": {
    "url": "https://github.com/linonetwo/styled-tachyons/issues"
  },
  "homepage": "https://github.com/linonetwo/styled-tachyons#readme",
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "^7.0.0-beta.55",
    "@babel/plugin-proposal-do-expressions": "^7.0.0-beta.55",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.55",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/preset-env": "^7.0.0-beta.55",
    "@babel/runtime": "^7.0.0-beta.55",
    "babel-eslint": "^9.0.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-macros": "^2.4.0",
    "chai": "^4.1.2",
    "eslint": "^5.5.0",
    "eslint-plugin-babel": "^5.2.0",
    "eslint-plugin-mocha": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "param.macro": "^2.1.0",
    "tachyons-js": "^0.3.1"
  }
}
